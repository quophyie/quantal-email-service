{
  "name": "quantal-email-service",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.0.0 "
  },
  "scripts": {
    "start": "npm run migrations && npm run seed && node dist/index",
    "seed": "node_modules/knex/bin/cli.js seed:run",
    "migrations": "node_modules/knex/bin/cli.js migrate:latest",
    "test": "npm run build && NODE_ENV=development node_modules/mocha/bin/mocha dist/test/**/*tests.js  ",
    "build": "rm -rvf ./dist && mkdir -p ./dist && node node_modules/babel-cli/bin/babel ./ --out-dir ./dist --ignore ./node_modules, ./dist",
    "build_tests": "npm run build && node node_modules/babel-cli/bin/babel test --out-dir dist/test --ignore node_modules, dist",
    "prepublish": "npm run build"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "aspect.js": "^0.5.1",
    "axios": "^0.15.3",
    "body-parser": "^1.16.1",
    "bookshelf": "^0.10.4",
    "bookshelf-camelcase": "^1.1.4",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-modelbase": "^2.10.4",
    "celebrate": "^7.0.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "create-error": "^0.3.1",
    "debug": "^2.6.9",
    "express": "^4.16.2",
    "express-enrouten": "^1.3.0",
    "joi": "^10.6.0",
    "knex": "^0.12.9",
    "lodash": "^4.17.4",
    "mailgun-js": "^0.14.2",
    "morgan": "^1.7.0",
    "pg": "^6.4.2",
    "quant-beat": "0.0.31",
    "quantal-errors": "0.0.14",
    "quantal-nodejs-shared": "0.0.66",
    "qute-bookshelf-repository": "0.0.2",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "code": "^4.0.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^4.2.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^2.1.1",
    "http-status": "^1.0.1",
    "mocha": "^3.5.3",
    "moment": "^2.20.1",
    "sinon": "^4.2.0",
    "sinon-stub-promise": "^4.0.0",
    "standard": "^9.0.1",
    "supertest": "^3.0.0",
    "supertest-promised": "^1.0.0"
  }
}
